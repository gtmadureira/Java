<?xml version="1.0" encoding="UTF-8"?>

<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 https://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="javafx-archetype-gtmadureira">

<fileSets>

<fileSet filtered="true" packaged="true" encoding="UTF-8">
    <directory>src/main/java</directory>
    <includes>
        <include>**/*.java</include>
    </includes>    
</fileSet>

<fileSet filtered="true" encoding="UTF-8">
    <directory>src/main/resources</directory>
    <includes>
	<include>**/*.fxml</include>
        <include>**/*.css</include>
        <include>log4j.properties</include>        
    </includes>    
</fileSet>

<fileSet filtered="true" encoding="UTF-8">
    <directory>.vscode</directory>
    <includes>
        <include>**/*.json</include>
    </includes>    
</fileSet>

<fileSet filtered="true" encoding="UTF-8">
    <directory>pkged-app</directory>
    <includes>
        <include>**/*</include>
    </includes>    
</fileSet>

<fileSet filtered="true" encoding="UTF-8">
    <directory>stub</directory>
    <includes>
        <include>**/*</include>
    </includes>    
</fileSet>

<fileSet filtered="true" encoding="UTF-8">
    <directory></directory>
    <includes>
        <include>MANIFEST.MF</include>
        <include>LICENSE</include>
	    <include>nb-configuration.xml</include>
	    <include>nbactions.xml</include>                
    </includes>
</fileSet>

</fileSets>

</archetype-descriptor>
